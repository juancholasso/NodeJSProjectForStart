<% if (locals.success) { %>
    <script type="text/javascript">
        console.log("entro")

    swal({
            title: '<%= locals.success.title %>',
            text: '<%= locals.success.msg %>',
            buttonsStyling: false,
            confirmButtonClass: "btn btn-success btn-fill",
            type: "success"
        });
    </script>
<% } %>

<% if (locals.error) { %>
    <script type="text/javascript">
    swal({
            title: '<%= locals.error.title %>',
            text: '<%= locals.error.msg %>',
            buttonsStyling: false,
            confirmButtonClass: "btn btn-warning btn-fill",
            type: "warning"
        });
    </script>
<% } %>

<% if (locals.time) { %>
    <script type="text/javascript">
    swal({ 
            title: '<%= locals.time.title %>',
            text: '<%= locals.time.msg %>',
            timer: '<%= locals.time.time %>',
            showConfirmButton: false
        });
    </script>
<% } %>